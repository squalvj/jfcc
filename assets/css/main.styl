@font-face
	font-family sectraB
	src url(../font/GT-Sectra-Fine-Book.ttf)
@font-face
	font-family sectraBI
	src url(../font/GT-Sectra-Fine-Book-Italic.ttf)


body
	background-color #F1F2F2
	font-size 12px
	-webkit-font-smoothing: antialiased;
	text-rendering: optimizeLegibility;
	font-feature-settings: "kern";
	-webkit-font-feature-settings: "kern";
	-moz-font-feature-settings: "kern";
	-moz-font-feature-settings: "kern=1";
	font-kerning: normal;
.content-mid
	margin 18px 0
.no-p
	padding 0
.no-m
	margin 0
.rel
	position relative
.no-p-left
	padding-left 0
.no-p-right
	padding-right 0
.abs
	position absolute
// home

.home-1
	background-color white
	padding 15px 0
.w100
	width 100%

.border-right
	border-right 1px solid rgba(0,0,0,0.1)

.desc
	font-size 11px
	font-family "Helvetica"
	text-transform uppercase
	-webkit-font-smoothing: auto;
	letter-spacing 0.7px
	margin-top 15px
	a
		color #ED1C24
.title
	a
		font-family sectraB
		color #414042
		font-size 32px
		font-weight lighter
		letter-spacing 0.5px
.p-content
	font-family "Helvetica"
	font-size 15px
	margin 15px 0
	color #414042
	letter-spacing: 0.3px;
.datetime
	color #939598
	font-family "Helvetica"
	font-size 12px
	margin-bottom 30px
.title2
	line-height 18px
	margin-top 0
	a
		font-size 20px
.title3
	margin-top 0
	line-height 14px
	a
		font-size 16px
.border-bottom
	border-bottom 1px solid rgba(0,0,0,0.1)
	&:last-child
		margin-top 13px
		border none
.shadow
	box-shadow: 0px 0px 35px -15px black;
.header-upcoming
	background-color black
	margin 0
	padding: 12px 15px;
	a
		color white
.wrapper-big-upcoming
	padding 1px 15px
	background-color #1C1B1C
	color white
	p
		font-size 16px
		font-weight lighter
		color #939598
		font-family "Helvetica"
		padding-bottom 20px
		padding 0
.big-upcoming
	margin 5px 0
	font-family sectraB
	font-size 45px
	line-height 50px
	.big
		font-size 85px
		font-family sectraB
.inner-desc
	padding 12px 15px
	margin 0
	background #F1F2F2
	font-weight bold
	a
		color #414042
.inner-upcoming
	padding 0 15px 12px
	background-color #f1f2f2
.date-upcoming
	margin-bottom 0
.inner-upcoming-inside
	padding 12px 0
.member-desc
	background-color black
	margin 0
	padding 12px 15px
	letter-spacing 1px
	color white
	
.wrapper-testimoni
	padding 12px 15px
	background-color #FFFCF8
	
.membership
	height 435px //xianjing
.wrapper-inside-testimoni
	border-radius 12px
	background-color #E7E5E1
	padding 16px 19px
	height 75%
	p
		font-family sectraBI
		font-size 18px
.red
	font-family "Helvetica"
	font-size 12px
	color #ED1C24
.circle-clip
	bottom: -50px;
	right: 20px;
	overflow: hidden;
	border-radius: 100%;
.member-benefit
	border-bottom 1px solid rgba(0,0,0,0.1)
	margin-top 0 !important
	background-color #F8E9CF
	text-transform none
.wrapper-benefit
	height: 435px;
	overflow hidden
.wrapper-right-benefit
	background-color #FFF4E3
	padding 14px 0
	div
		&:first-child
			padding-right 7px
		&:last-child
			padding-left 7px
.title-benefit
	margin 12px 0
.desc-benefit
	font-size 12px
	font-family "Helvetica"
.btn-join
	color white
	background-color #ED1C24
	margin-top 48px
	margin-bottom 5px
	font-family "Helvetica"
	font-weight bolder